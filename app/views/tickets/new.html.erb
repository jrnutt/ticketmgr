<h1>Odd Job Computing Ticketing System</h1>

<%= form_for :ticket, url: tickets_path do |f| %>
<p>
  <%= f.label :issue %><br>
  <%= f.text_field :issue %>
</p>

<p>
  <%= f.label :client_email %><br>
  <%= f.email_field :client_email %>
</p>

<p>
  <%= f.label :submit_date %><br>
  <%= f.date_field :submit_date %>
</p>

<p>
  <%= f.label :description %><br>
  <%= f.text_area :description %>
</p>

<p>
  <%= f.submit %>
</p>
<% end %>
